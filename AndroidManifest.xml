    <application android:label="@string/app_name" android:icon="@drawable/ic_launcher">
        <activity android:name="com.xuwt.boundservice.MainActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".LocalActivity"></activity>
        <activity android:name=".MessengerActivity"></activity>
        <activity android:name=".MessengerCommunicateActivity"></activity>
        <activity android:name=".aidl.RemoteActivity"></activity>
        <service android:name=".MessengerService"></service>
        <service android:name=".MessengerCommunicateService"></service>
        <service android:name=".LocalService"></service>
        <service android:name=".aidl.RemoteService"></service>
    </application>
</manifest>
